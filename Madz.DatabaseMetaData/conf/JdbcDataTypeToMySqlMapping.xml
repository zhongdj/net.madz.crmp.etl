		<property-binding jdbc-name="size"
			constructor-argument-type="java.lang.Integer" constructor-argument="length" />
		<property-binding jdbc-name="decimalDigits"
			constructor-argument-type="java.lang.Integer" constructor-argument="decimals" />
	</jdbc-data-type-binding>
