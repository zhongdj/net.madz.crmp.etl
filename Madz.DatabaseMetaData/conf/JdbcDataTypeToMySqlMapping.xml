	xsi:schemaLocation="http://www.example.org/JdbcDataTypeToMySqlMappingSchema JdbcDataTypeToMySqlMappingSchema.xsd">
	<jdbc-data-type-binding id="-11" name="GUID"
		data-type-class="net.madz.db.core.meta.immutable.mysql.datatype.MySQLChar"
		maxLength="255">
		<property-binding jdbc-name="size"
			constructor-argument="length" constructor-argument-type="java.lang.Long"
			datatype-convertor-clzName="net.madz.db.utils.IntegerToLongConvertor" />
	</jdbc-data-type-binding>
	<jdbc-data-type-binding id="-7" name="BIT"
		data-type-class="net.madz.db.core.meta.immutable.mysql.datatype.MySQLBit">
